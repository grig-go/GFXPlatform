# Pulsar Hub Environment Variables

# Supabase Configuration
VITE_PULSAR_HUB_SUPABASE_URL=https://your-project.supabase.co
VITE_PULSAR_HUB_SUPABASE_ANON_KEY=your-anon-key

# App URLs (for launching other Pulsar apps)
VITE_PULSAR_GFX_URL=https://your-pulsar-gfx-url.netlify.app
VITE_PULSAR_VS_URL=https://your-pulsar-vs-url.netlify.app
VITE_PULSAR_MCR_URL=https://your-pulsar-mcr-url.netlify.app
VITE_NEXUS_URL=https://your-nexus-url.netlify.app

# Development port (optional, defaults to 3005)
VITE_PULSAR_HUB_PORT=3005
